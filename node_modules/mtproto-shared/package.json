{
  "name": "mtproto-shared",
  "version": "0.1.8",
  "description": "Core utils for telegram-mtproto",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "clean": "rimraf es/ && rimraf lib/",
    "prepublish": "npm run clean && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "types": "flow-copy-source src lib && flow-copy-source src es",
    "watch:cjs": "npm run build:cjs -- --watch"
  },
  "files": [
    "lib",
    "src",
    "es",
    "index.d.ts"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "memoizee": "^0.4.5"
  },
  "devDependencies": {
    "@types/memoizee": "^0.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "cross-env": "^5.0.5",

    "flow-copy-source": "^1.2.0",
    "rimraf": "^2.6.1"
  },
  "author": "Zero Bias",
  "license": "MIT"
}
